  "author": "GitHub",
  "license": "CC0-1.0",
  "dependencies": {
    "jquery": "^3.3.1",
    "mongoose": "^5.0.6",
    "mongoose-paginate": "^5.0.3"
  },
  "devDependencies": {
    "electron": "^1.8.2",
    "electron-reload": "^1.2.2"
  }
}
